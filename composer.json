  "extra": {
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patches": {
      "drupal/core": {
        "Dialog drupalAutoButtons option should be respected on initial load": "https://git.drupalcode.org/project/drupal/-/merge_requests/222.patch",
        "Changing an existing embedded media's alignment or alt data attributes does not get saved with CKEditor": "https://www.drupal.org/files/issues/2020-06-01/3102249-21.patch",
        "There should be no output of default formatter for empty text field when summary has content": "https://www.drupal.org/files/issues/2018-07-19/empty_text_formatter_for_empty_fields-2309585-13.patch",
        "Site lock and basic auth combo": "https://www.drupal.org/files/issues/basic_auth_0.patch"
      },
      "drupal/layout_library": {
        "Form blocks rendered inside layout builder library break save": "https://www.drupal.org/files/issues/2020-05-04/layout-library-save-issue-3133484-4.patch"
      },
      "drupal/default_content": {
        "Add a Normalizer and Denormalizer to support Layout Builder": "https://www.drupal.org/files/issues/2020-08-24/default_content-add-normalizer-denormalizer-for-layout-builder-3160146-25.patch"
      },
      "drupal/redirect": {
        "3037259 - Endless Redirection loop when using Domain access": "https://git.drupalcode.org/project/redirect/-/merge_requests/31.patch"
      }
    },
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/sites/default/settings.php": {
          "path": "assets/settings.php",
          "overwrite": false
        },
        "[web-root]/sites/default/settings/settings.main.php": "assets/settings/settings.main.php",
        "[web-root]/sites/default/settings/settings.dev.php": "assets/settings/settings.dev.php",
        "[web-root]/sites/default/settings/settings.lando.php": "assets/settings/settings.lando.php",
        "[web-root]/sites/default/settings/settings.live.php": "assets/settings/settings.live.php",
        "[web-root]/sites/default/settings/settings.local.php": {
          "path": "assets/settings/settings.local.php",
          "overwrite": false
        },
        "[web-root]/sites/default/settings/settings.pantheon.php": "assets/settings/settings.pantheon.php",
        "[web-root]/sites/default/settings/settings.test.php": "assets/settings/settings.test.php",
        ".github/scripts/build.sh": "assets/.github/scripts/build.sh",
        ".github/scripts/synctags.sh": "assets/.github/scripts/synctags.sh",
        ".github/workflows/pantheon_deploy.yml": "assets/.github/workflows/pantheon_deploy.yml",
        ".github/workflows/synctags.yml": "assets/.github/workflows/synctags.yml",
        "[web-root]/sites/aliases.php": {
          "path": "assets/aliases.php",
          "overwrite": false
        },
        "[web-root]/sites/development.services.yml": "assets/development.services.yml",
        ".lando/artefact.sh": "assets/.lando/artefact.sh",
        ".lando/fresh-install.sh": "assets/.lando/fresh-install.sh",
        ".lando/setup_terminus.sh": "assets/.lando/setup_terminus.sh",
        ".lando/verify_mac_ssh_socket.sh": "assets/.lando/verify_mac_ssh_socket.sh",
        ".gitignore": "assets/gitignore",
        "pantheon.upstream.yml": {
          "path": "assets/pantheon.upstream.yml",
          "overwrite": false
        },
        "pantheon.yml": {
          "path": "assets/pantheon.yml",
          "overwrite": false
        },
        "[web-root]/private/scripts/deploy_after.php": "assets/deploy_after.php"
      }
    }
  },
  "require-dev": {
    "drupal/core-composer-scaffold": "^9.4",
    "drupal/coder": "^8.3"
  }
