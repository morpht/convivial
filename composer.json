    "drupal/coder": "^8.3",
    "enlightn/security-checker": "^1.11",
    "mglaman/drupal-check": "^1.4",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpro/grumphp": "^2.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "oomphinc/composer-installers-extender": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true
    },
    "bin-dir": "bin/",
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "enable-patching": true,
    "patches": {
      "drupal/core": {
        "2309585 - There should be no output of default formatter for empty text field when summary has content": "https://www.drupal.org/files/issues/2018-07-19/empty_text_formatter_for_empty_fields-2309585-13.patch",
        "2842858 - Site lock and basic auth combo": "https://www.drupal.org/files/issues/basic_auth_0.patch",
        "3326684 - Deprecated function mb_strtolower passing null to parameter": "https://www.drupal.org/files/issues/2023-09-14/3326684-22.patch",
        "3328425 - CKEditor 5 balloons invisible when CKEditor 5 is used inside a modal": "https://www.drupal.org/files/issues/2022-12-27/3328425.patch",
        "3392572 - Deprecated function: strnatcasecmp()": "https://www.drupal.org/files/issues/2024-02-09/3392572-40-10.2.patch"
      },
      "drupal/layout_library": {
        "3133484 - Form blocks rendered inside layout builder library break save": "https://www.drupal.org/files/issues/2020-05-04/layout-library-save-issue-3133484-4.patch"
      },
      "drupal/layout_paragraphs": {
        "3417765 - Large modal exceeds viewport height without scrollbar = actions unreachable": "https://www.drupal.org/files/issues/2024-03-28/large-modal-exceeds-viewport-3417765-8.patch"
      },
      "drupal/redirect": {
        "3037259 - Endless Redirection loop when using Domain access": "https://www.drupal.org/files/issues/2022-09-13/3037259-28.patch"
      }
    }
  }
}
