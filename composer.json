        },
        "[web-root]/sites/default/settings/settings.pantheon.php": "assets/settings/settings.pantheon.php",
        "[web-root]/sites/default/settings/settings.test.php": "assets/settings/settings.test.php",
        ".github/scripts/build.sh": "assets/.github/scripts/build.sh",
        ".github/scripts/synctags.sh": "assets/.github/scripts/synctags.sh",
        ".github/workflows/pantheon_deploy.yml": "assets/.github/workflows/pantheon_deploy.yml",
        ".github/workflows/synctags.yml": "assets/.github/workflows/synctags.yml",
        "[web-root]/sites/aliases.php": {
          "path": "assets/aliases.php",
          "overwrite": false
        },
        "[web-root]/sites/development.services.yml": "assets/development.services.yml",
        ".lando/artefact.sh": "assets/.lando/artefact.sh",
        ".lando/fresh-install.sh": "assets/.lando/fresh-install.sh",
        ".lando/setup_terminus.sh": "assets/.lando/setup_terminus.sh",
        ".lando/verify_mac_ssh_socket.sh": "assets/.lando/verify_mac_ssh_socket.sh",
        ".gitignore": "assets/gitignore",
        "pantheon.upstream.yml": {
          "path": "assets/pantheon.upstream.yml",
          "overwrite": false
        },
        "pantheon.yml": {
          "path": "assets/pantheon.yml",
          "overwrite": false
        },
        "[web-root]/private/scripts/deploy_after.php": "assets/deploy_after.php"
      }
    }
